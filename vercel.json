{
  "version": 2,
  "builds": [
    {
      "src": "B/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "forentend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "B/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "forentend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}